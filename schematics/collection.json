{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "init-project": {
      "description": "Init an ArgoCD blank project",
      "factory": "./init-project/index#init"
    },
    "add-app": {
      "description": "add an application into project",
      "factory": "./add-app/index#add"
    },
    "copy-app": {
      "description": "copy app from project to other project",
      "factory": "./copy-app/index#command"
    }
  }
}
